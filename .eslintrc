{
  "parserOptions": {
    "ecmaVersion": 6,
    "sourceType": "module",
    "ecmaFeatures": {
      "jsx": true
    }
  },
  "extends": [
    "eslint:recommended",
    "eslint-config-prettier",
    "react-app"
  ],
  "rules": {
    "strict": [
      "error",
      "never"
    ]
  },
  "env": {
    "browser": true
  }
}